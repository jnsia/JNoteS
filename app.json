{
  "expo": {
    "name": "러브미!션",
    "slug": "JNoteS",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#FF231F7C"
    },
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2a2d3e"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/ca363540-d8ae-4c34-8bb6-e7dedbea0f99"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 2,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#2a2d3e"
      },
      "package": "com.jnsia.JNoteS",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Pretendard-Regular.ttf", "./assets/fonts/Pretendard-Bold.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ca363540-d8ae-4c34-8bb6-e7dedbea0f99"
      }
    },
    "owner": "jnsia",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-3115269616339333~1662364869"
  }
}
